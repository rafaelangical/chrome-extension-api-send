{
  "name": "API data message",
  "manifest_version": 2,
  "version": "2.1",
  "description": "Send data api message for phone.",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": [
    "https://*.google.com/",
    "activeTab",
    "http://fastzap.site/registrar/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
